<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">
    <model name="closed_chain">
      <pose>0. 0 0.0 0 0 0</pose>

      <!-- Include the Panda robot model -->
      <include>
        <uri>/home/cory/Documents/drake_brubotics/models/descriptions/robots/panda_fr3/urdf/panda_fr3_nohand.urdf</uri> 
      </include>

      <!-- Include the suspended brick model -->
      <include>
        <uri>/home/cory/Documents/drake_brubotics/models/descriptions/hybrids/crane/URDF/rope_model.sdf</uri> 
      </include>
      
      <!-- Joint to fix the Panda  to the crane -->
    <joint name="panda_to_block" type="revolute">
      <child>example_rope::example_rope_brick</child>
      <parent>panda::panda_link0</parent>
      <axis>
        <xyz expressed_in="__model__">0 1 0</xyz>
        <limit>
          <!-- An effort of 0 is parsed as an unactuated joint. -->
          <effort>0</effort>
        </limit>
        <dynamics>
          <damping>0.0</damping>
        </dynamics>
      </axis>
    </joint>
    </model>
  </world>
</sdf>
